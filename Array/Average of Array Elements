import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr[] = new int[n];
        int sum=0;
        int count=0;
        for(int i=0; i<n; i++){
            arr[i] = sc.nextInt();
            count++;
            sum+=arr[i];
        }
        System.out.println(sum/count);
            
    }
}
