import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int arr[]= new int[256];
        String s= sc.nextLine();
        String t="";
        int n= s.length();
        for(int i=0;i<n;i++){
            char c=s.charAt(i);
            if(t.indexOf(c)==-1){
                t+=c;
            }
        }
        System.out.println(t);
    }   
}
