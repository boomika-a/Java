import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        String s= sc.nextLine();
        int n= s.length();
        boolean f= true;
        for(int i=0;i<n;i++){
            char c= s.charAt(i);
            if(!(c >='0' && c<='9'))
                f= false;
        }
        if(f==true)
           System.out.println("YES");
        else
           System.out.println("NO");
    }
}
