import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        String s= sc.nextLine();
        String org="";
        int n= s.length();
        for(int i=n-1;i>=0;i--){
            org+=s.charAt(i);
        }
        if(org.equals(s))
            System.out.println("YES");
        else
            System.out.println("NO");
    }
}
