import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
        int r = sc.nextInt();
        int c= sc.nextInt();
        int arr[][]= new int[r][c];
        for(int i=0;i<r;i++){
         for(int j=0;j<c;j++){
            arr[i][j]=sc.nextInt();
         }
        }
        for(int i=0;i<r;i++){
             int ce=0; 
             int co=0;
         for(int j=0;j<c;j++) { 
             if(arr[i][j]%2==0)
                 ce++;
            else
                co++;
        }
             System.out.print(ce +" "+co);
             System.out.println();
    }
   
    }
  }    
